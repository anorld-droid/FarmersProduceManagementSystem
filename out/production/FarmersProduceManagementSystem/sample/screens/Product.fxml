<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane  xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <Button fx:id="productsBtn" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onAction="#Products" text="Products" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="20.0">
        <font>
            <Font size="14.0" />
        </font>
    </Button>
    <Button fx:id="farmersBtn" layoutX="85.0" layoutY="20.0" mnemonicParsing="false" onAction="#Farmers" text="Farmers" AnchorPane.leftAnchor="85.0" AnchorPane.topAnchor="20.0">
        <font>
            <Font size="14.0" />
        </font>
    </Button>
    <Button fx:id="retailersBtn" layoutX="155.0" layoutY="20.0" mnemonicParsing="false" onAction="#Retailers" text="Retailers" AnchorPane.leftAnchor="155.0" AnchorPane.topAnchor="20.0">
        <font>
            <Font size="14.0" />
        </font>
    </Button>
    <TableView fx:id="productTbl"   layoutX="10.0" layoutY="41.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="17.0" AnchorPane.topAnchor="50.0">
         <columns>
             <TableColumn fx:id="id" prefWidth="30.0" text="ID" resizable="false"/>
             <TableColumn fx:id="name" minWidth="100.0" text="Name" resizable="false"/>
             <TableColumn fx:id="quantity" minWidth="150.0" text="Quantity" resizable="false" />
             <TableColumn fx:id="expiryDate" minWidth="100.0" prefWidth="200.0" text="Expiry Date" />
         </columns>
    </TableView>
</AnchorPane>
